@{
    ViewBag.Title = "订单管理"; 
}
@section css{
    <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
}
@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/faudit/ordersaudit"], function (ObjectJS) {
            ObjectJS.init();
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">订单审核</span> 
</div>
<div class="search-header">
    <div class="left mRight5" id="orderType"></div>
    <div class="left mRight5" id="orderStatus"></div>
    <div class="left" title="创建时间">
        <input type="text" id="iptCreateTime" placeholder="创建时间" class="ipt-date"  />
    </div>
    <div class="btn-filter right"></div>
    <div class="right searth-module mRight20" data-text="关键字搜索..." data-width="160"> </div>
</div>

<div class="content-body">
    <table class="table-list">
        <tr class="tr-header">
            <td class="width30"></td>
            <td class="tLeft width150">订单编号</td>
            <td class="tLeft minwidth70">商品名称</td>
            <td class="tLeft minwidth70">内容</td>
            <td class="width80">总金额</td>
            <td class="width80">支付方式</td>
            <td class="width80">状态</td>
            <td class="width150">创建时间</td>
            <td class="width100">创建人</td>
            <td class="width60">操作</td>
        </tr>
    </table>
    <ul class="left batch-operate mTop20 hide">
        <li class="width100 mTop5">
            <span class="ico-check pLeft5" id="checkAll">批量操作</span>
        </li>
        <li class="width60">
            <a href="javascript:void(0);" class="btn mulitImgUpdate" data-type="1">通过</a>
        </li>
        <li class="width60">
            <a href="javascript:void(0);" class="btn mulitImgUpdate" data-type="6">驳回</a>
        </li>
    </ul>
    <div id="pager" class="mTop20"></div>
    <div class="clear"></div>
</div>
<ul class="dropdown-ul hide" style="width:65px;">
    <li class=" " id="updateObject">审核</li>
    <li class=" " id="deleteObject">作废</li>
</ul>

