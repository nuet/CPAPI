@{
    ViewBag.Title = "租友信息管理"; 
}

@section scripts{
    <script type="text/javascript">
        seajs.use(["scripts/sysset/webusers"], function (obj) {
            obj.init('');
        });
    </script>
}
<div class="header-box">
    <span class="header-title left">租友信息管理</span> 
</div>
<div class="search-header"> 
    <div class="left mRight5" id="userStatus"></div>  
    <div class="left searth-module" data-text="关键字搜索..." data-width="160"> </div>
</div>
<div class="content-body">
    <table class="table-list">
        <tr class="tr-header">
            <td class="tLeft">昵称名称</td>
            <td class="tLeft">登陆账号</td>
            <td class="tLeft">状态</td>
            <td class="width150">注册时间</td>
            <td class="width80">操作</td>
        </tr>
    </table>
    <div id="pager" class="mTop20"></div>
</div>
<ul class="dropdown-ul hide" style="width:65px;">
    <li class=" " id="updateObject" title="允许登陆">解禁</li>
    <li class=" " id="deleteObject" title="禁止登录">禁闭</li>
</ul>
