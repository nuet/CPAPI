@{
    ViewBag.Title = "意见反馈";
    Layout = null;
}
<html>
<head>
    <meta http-equiv="content-type" content="text/html" charset="utf-8" />
    <link href="/modules/css/base.css" rel="stylesheet" />
    <link href="/modules/css/layout.css" rel="stylesheet" />
    <link href="/modules/css/iconfont/iconfont.css" rel="stylesheet" />
    <link href="/modules/plug/daterangepicker/daterangepicker.css" rel="stylesheet" />
    <script src="/Scripts/jquery-1.11.1.js"></script>
    <script type="text/javascript" src="/Scripts/sea.js"></script>
    <script type="text/javascript" src="/Scripts/sea-config.js"></script>
    <script type="text/javascript">
        seajs.use(["scripts/sysset/feedback"], function (feedback) {
            feedback.init();
        });

    </script>
</head>
<body>
    <div class="main-body">
        <div class="main-content">
            <div class="header-box">
                <span class="header-title">当前位置： >意见反馈</span>
            </div>

            <div class="search-header">
                <ul class="search-tab search-status" style="margin-left: 0px !important;">
                    <li class="hover" data-index="-1">所有</li>
                    <li data-index="0">待解决</li>
                    <li data-index="1">已解决</li>
                    <li data-index="2">驳回</li>
                    <li data-index="9">删除</li>
                </ul>
            </div>

            <div class="content-body">
                <div class="search-body">
                    <div id="FeedTypes" class="left"></div>
                    <div class="left mLeft20" title="创建时间">
                        <input type="text" id="iptCreateTime" placeholder="创建时间" class="ipt-date" />
                    </div>
                    <div class="right searth-module" data-text="输入关键字..." data-width="150"></div>
                    <div class="clear"></div>
                </div>

                <table class="table-list">
                    <tr class="tr-header">
                        <td class="tLeft">标题</td>
                        <td class="tLeft">类型</td>
                        <td class="tLeft">状态</td>
                        <td class="tLeft">联系人</td>
                        <td class="tLeft">电话</td>
                        <td class="tLeft">创建时间</td>
                        <td class="tLeft" width="10%">操作</td>
                    </tr>
                </table>

                <div id="pager" class="mTop20"></div>
            </div>


        </div>
    </div>
</body>
</html>
